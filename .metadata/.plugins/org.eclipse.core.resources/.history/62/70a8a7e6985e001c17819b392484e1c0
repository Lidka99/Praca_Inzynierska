package application.view.intermediate;

import java.util.Date;

public class TimeRaportEntry {

	public String dateString;
	public Date date;
	public Float minTime;
	public Float maxTime;
	public Float avgTime;
	

	public String toCSV() {
		return date.toString() + ";" + minTime + ";" + maxTime + ";" + avgTime;
	}

	public TimeRaportEntry(Date date, Float minTime, Float maxTime, Float avgTime) {
		super();
		this.date = date;
		this.dateString = date.toString();
		this.minTime = minTime;
		this.maxTime = maxTime;
		this.avgTime = avgTime;
		
		
	}

	public String getDateString() {
		return dateString;
	}

	public Date getDate() {
		return date;
	}

	public Float getMinTime() {
		return minTime;
	}

	public Float getMaxTime() {
		return maxTime;
	}

	public Float getAvgTime() {
		return avgTime;
	}

	public String getMinTimeString() {
		return minTimeString;
	}

	public String getMaxTimeString() {
		return maxTimeString;
	}

	public String getAvgTimeString() {
		return avgTimeString;
	}
	
	

}
