<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.Application_view_controller">
   <top>
      <ToolBar prefHeight="32.0" prefWidth="900.0" BorderPane.alignment="CENTER">
        <items>
          <Button fx:id="main_page_button" mnemonicParsing="false" onAction="#onMainPageButtonClick" text="Strona główna" />
            <Button fx:id="raport_button" mnemonicParsing="false" onAction="#onRaportButtonClick" text="Raport" />
            <Button mnemonicParsing="false" text="Button" />
        </items>
      </ToolBar>
   </top>
</BorderPane>
